{
    "name": "rauk-inventory/rauk-inventory-php",
    "description": "PHP SDK for Rauk Inventory API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "RaukInventory\\": "src/"
        },
        "classmap": [
            "src/Types/Operations.php",
            "src/Types/InventoryItem.php",
            "src/Utils/Errors.php",
            "src/Utils/SignRequest.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RaukInventory\\Tests\\": "tests/units/",
            "RaukInventory\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "check": [
            "@test",
            "@phpstan"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}